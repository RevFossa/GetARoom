<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/bckgrnd"
    tools:context="com.example.hotelreseration.MainScreenActivity$PlaceholderFragment" >

    <View
        android:id="@+id/View01"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/changeinfo"
        android:background="#000000"
        android:visibility="visible"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/user_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Information"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <View
        android:id="@+id/View02"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/View01"
        android:layout_below="@+id/guide"
        android:background="#000000"
        android:visibility="visible"
        tools:ignore="RtlHardcoded" />

    <View
        android:id="@+id/View06"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignLeft="@+id/View04"
        android:layout_alignRight="@+id/View04"
        android:layout_below="@+id/reportaproblem"
        android:background="#000000"
        android:visibility="visible"
        tools:ignore="RtlHardcoded" />

    <View
        android:id="@+id/View05"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/contactus"
        android:background="#000000"
        android:visibility="visible"
        tools:ignore="RtlHardcoded" />

    <View
        android:id="@+id/View08"
        android:layout_width="wrap_content"
        android:layout_height="2dp"
        android:layout_alignBottom="@+id/user_label"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="#000000"
        android:visibility="visible"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/reportaproblem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/user_label"
        android:layout_below="@+id/View05"
        android:layout_marginTop="50dp"
        android:text="Report A Problem"
        android:textAppearance="?android:attr/textAppearanceLarge"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/contactus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:text="Contact Us"
        android:textAppearance="?android:attr/textAppearanceLarge"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/View02"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/View01"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="57dp"
        android:text="Owner Guide"
        android:textAppearance="?android:attr/textAppearanceLarge"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/changeinfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About Us"
        android:textAppearance="?android:attr/textAppearanceLarge"
        tools:ignore="HardcodedText"
        android:layout_marginTop="61dp"
        android:layout_below="@+id/user_label"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_call"
        android:layout_above="@+id/View05"
        android:layout_alignStart="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_set_as"
        android:layout_alignBottom="@+id/View06"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp" />

    <ImageView
        android:id="@+id/userImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_info_details"
        android:layout_alignBottom="@+id/View01"
        android:layout_alignStart="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_crop"
        android:layout_alignBottom="@+id/View02"
        android:layout_alignStart="@+id/imageView3" />

</RelativeLayout>